<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mrschmitz.jobs.Activities.JobListActivity">

    <ListView
        android:id="@+id/viewJobs"
        android:layout_width="368dp"
        android:layout_height="495dp"
        app:layout_constraintBottom_toTopOf="@+id/back2google"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/back2google"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Back"
        android:onClick="backgoogle"
        tools:layout_editor_absoluteY="2dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"

        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewJobs"
        app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>

